{
  "name": "react-redux-form-starter",
  "version": "0.0.1",
  "description": "React Redux Form Starter",
  "main": "src/app.js",
  "private": true,
  "scripts": {
    "autoprefix": "postcss -c postcss.config.json",
    "prebuild:app": "rimraf dist && mkdir dist",
    "build:sass": "node-sass ./src/app.scss > ./dist/app.css",
    "postbuild:sass": "npm run autoprefix",
    "build:html": "cpy ./src/index.html ./dist",
    "build:app": "npm run lint && npm run build:sass && npm run build:html",
    "build": "npm run build:app && webpack",
    "build:prod": "npm run build:app && cross-env NODE_ENV=production webpack",
    "start": "npm run build:app && cross-env NODE_ENV=hot node server.js",
    "lint": "eslint src",
    "csslint": "csslintnext ./dist/app.css",
    "test:browser": "karma start --single-run --browsers Firefox --no-auto-watch",
    "test": "npm run build:prod && npm run test:browser",
    "watch:test": "karma start --debug",
    "report-coverage": "cat ./coverage/report-lcov/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "config": {
    "devTools": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mdgbayly/react-redux-form-starter"
  },
  "keywords": [
    "react",
    "redux",
    "react-redux-form"
  ],
  "author": "Martin Bayly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdgbayly/react-redux-form-starter/issues"
  },
  "homepage": "https://github.com/mdgbayly/react-redux-form-starter#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "coveralls": "^2.11.6",
    "cpy": "^3.4.1",
    "cross-env": "^1.0.7",
    "csslint-next": "^1.1.1",
    "enzyme": "^2.0.0",
    "eslint": "^1.10.3",
    "eslint-config-brightspace": "^0.1.0",
    "eslint-plugin-react": "^3.11.3",
    "expect": "^1.13.4",
    "expect-jsx": "^2.2.2",
    "history": "^1.13.1",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "postcss-cli": "^2.5.1",
    "react-addons-test-utils": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-mock-store": "0.0.6",
    "rimraf": "^2.4.2",
    "sinon": "^2.0.0-pre",
    "unexpected": "^10.9.1",
    "unexpected-react-shallow": "^0.7.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "array.prototype.find": "^2.0.0",
    "array.prototype.findindex": "^2.0.0",
    "babel-polyfill": "^6.6.1",
    "react": "^0.14.3",
    "react-addons-update": "^0.14.7",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.6",
    "react-redux-form": "^0.8.14",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.3",
    "string.prototype.startswith": "^0.2.0"
  }
}
